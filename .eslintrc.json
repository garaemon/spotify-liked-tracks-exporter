{
  "env": {
    "browser": true,
    "node": true,
    "es2020": true
  },
  "extends": ["./node_modules/gts", "prettier"],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error"
  },
  "ignorePatterns": ["template/**/*", "template-ui/**/*"]
}
